		<!--  Safari -->
		<device user_agent="Safari/" fall_back="generic_web_browser"
			id="safari">
			<group id="product_info">
				<capability name="brand_name" value="safari" />
			</group>
		</device>
		<device user_agent="Safari/525" fall_back="safari" id="safari_525">
			<group id="product_info">
